      <collision>
        <geometry>
          <mesh filename="package://dsr_description/meshes/m1013_collision/MF1013_2_1.stl" scale="0.001 0.001 0.001" />
        </geometry>
      </collision>
      <visual>
        <geometry>
          <mesh filename="package://dsr_description/meshes/m1013_white/MF1013_2_2.dae" scale="0.001 0.001 0.001" />
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dsr_description/meshes/m1013_collision/MF1013_2_2.stl" scale="0.001 0.001 0.001" />
        </geometry>
      </collision>
    </link>
    <!-- joint3 -->
    <joint name="${prefix}joint3" type="revolute">
      <parent link="${prefix}link2" />
      <child link="${prefix}link3" />
