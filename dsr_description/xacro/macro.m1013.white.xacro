    </link>
                
    <!-- TODO ROS base_link to Doosan Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="${prefix}base_link"/>
    <child link="${prefix}base"/>
    </joint>

    <!-- TODO ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange" />
    <joint name="${prefix}joint6-flange" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link6" />
    <child link="${prefix}flange" />
    </joint>
                
    <!-- TODO ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}link6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}flange" />
      <child link="${prefix}tool0" />
    </joint>
        
    <xacro:dsr_transmission />
    <xacro:dsr_gazebo namespace="${namespace}" />
