        </geometry>
      </collision>
    </link>
    <!-- joint6 -->
    <joint name="${prefix}joint6" type="revolute">
      <parent link="${prefix}link5" />
      <child link="${prefix}link6" />
      <origin rpy="${pi/2} 0 0" xyz="0 -0.121 0" />
      <axis xyz="0 0 1" />
      <limit effort="50" lower="-6.2832" upper="6.2832" velocity="3.927"/>
