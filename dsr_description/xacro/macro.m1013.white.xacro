    </joint>
    <link name="${prefix}link4">
      <inertial>
        <mass value="3.82"/>
        <origin xyz="7.1e-05    0.091879    -0.18252"/>
        <inertia ixx="0.19063" ixy="-1.8037e-05" ixz="-3.8576e-05" iyy="0.17268" iyz="-0.052996"
